    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.76.5",
    "postcss": "^8.4.24",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "styled-components": "^5"
  }
