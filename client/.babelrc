{
  // "presets": [
  //       "env",
  //       "react"
  //   ],
  "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "Components": "./src/components",
          "Pages": "./src/pages",
          "Routes": "./src/routes",
          "Hooks": "./src/hooks",
          "Utils": "./src/utils",
          "Api": "./src/api",
          "Datas": "./src/datas",
          "Redux": "./src/redux",
          "Actions": "./src/redux/actions"
        }
      }],
      [
        "babel-plugin-transform-imports",
        {
          "@material-ui/core": {
            "transform": "@material-ui/core/${member}",
            "preventFullImport": true
          }
        }
      ]
    ]
  }